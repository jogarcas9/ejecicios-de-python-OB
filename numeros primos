#Escribe una función que pueda decirte si un número (número entero) es primo o no.
numero = int(input('ingrese un numero entero: '))


def es_primo(num):
    for n in range(2, num):
        if num % n == 0:
            return ("No es primo", n, "es divisor")

    return 'Es primo'

print(es_primo(numero))
